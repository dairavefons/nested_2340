{
  "version": 2,
  "buildCommand": "bash build.sh",
  "builds": [
    {
      "src": "nestd/wsgi.py",
      "use": "@vercel/python",
      "config": { "maxLambdaSize": "15mb", "runtime": "python3.12" }
    }
  ],
  "routes": [
    {
      "src": "/favicon.ico",
      "dest": "/staticfiles/favicon.ico"
    },
    {
      "src": "/favicon.png",
      "dest": "/staticfiles/favicon.png"
    },
    {
      "src": "/static/(.*)",
      "dest": "/staticfiles/$1"
    },
    {
      "src": "/(.*)",
      "dest": "nestd/wsgi.py"
    }
  ]
}
